<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.maple.music.entity.User">
        <table name="m_user" schema="ssh_music" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="username">
                <column name="username"/>
            </basic>
            <basic name="password">
                <column name="password"/>
            </basic>
            <basic name="nickname">
                <column name="nickname"/>
            </basic>
            <basic name="creatTime">
                <column name="creat_time"/>
            </basic>
            <basic name="lastTime">
                <column name="last_time"/>
            </basic>
            <basic name="state">
                <column name="state"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
