<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!--建立类与表的映射关系-->
    <class name="com.maple.music.entity.Songs" table="m_Songs">
        <!--建立类中的属性与表中的主键映射-->
        <id name="id" column="id">
            <generator class="native"></generator>
        </id>
        <!--建立类中的普通属性与表中的属性映射-->
        <property name="name" column="name"/>
        <property name="songId" column="song_id"/>
        <property name="avatorId" column="avator_id"/>
        <property name="alia" column="alia"/>
        <property name="albumId" column="album_id"/>
        <property name="avatorNames" column="avator_names"/>
        <property name="dateTime" column="date_time"/>
        <property name="songUrl" column="song_url"/>
        <property name="lrc" column="lrc"/>
        <property name="playCount" column="play_count"/>
        <property name="oldPlayCount" column="old_play_count"/>
        <property name="lastWeekPlayCount" column="last_week_play_count"/>
        <property name="createDate" column="create_date"/>
        <property name="updateDate" column="update_date"/>
    </class>
</hibernate-mapping>